<p id="notice"><%= notice %></p>

<h1>Listing Posts Likes</h1>

<table>
  <thead>
    <tr>
      <th>Post</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @posts_likes.each do |posts_like| %>
      <tr>
        <td><%= posts_like.post_id %></td>
        <td><%= posts_like.user_id %></td>
        <td><%= link_to 'Show', posts_like %></td>
        <td><%= link_to 'Edit', edit_posts_like_path(posts_like) %></td>
        <td><%= link_to 'Destroy', posts_like, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Posts like', new_posts_like_path %>
